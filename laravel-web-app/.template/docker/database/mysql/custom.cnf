[mysqld]
# General
user = mysql
default-storage-engine = InnoDB

# Memory
innodb_buffer_pool_size = 256M
innodb_log_file_size = 64M
innodb_log_buffer_size = 8M
innodb_file_per_table = 1
innodb_flush_log_at_trx_commit = 2

# Connections
max_connections = 100

[mysqldump]
quick
quote-names
max_allowed_packet = 16M
column-statistics = 0
set-gtid-purged = OFF

[isamchk]
key_buffer = 16M
